{
  "name": "btl-web-2",
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd my-web-app/backend && npm run dev",
    "client": "cd my-web-app/frontend && npm start",
    "install-server": "cd my-web-app/backend && npm install",
    "install-client": "cd my-web-app/frontend && npm install",
    "install-all": "npm run install-server && npm run install-client"
  },
  "devDependencies": {
    "concurrently": "^7.6.0"
  }
}